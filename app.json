{
	"expo": {
		"name": "belshore-app",
		"slug": "belshore-drive",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "com.belshoredrive",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.belshoredrive.app",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false,
				"NSLocalNotificationUsageDescription": "This app needs access to send notifications to you."
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"permissions": [
				"android.permission.READ_CONTACTS",
				"android.permission.WRITE_CONTACTS",
				"android.permission.RECORD_AUDIO",
				"android.permission.CAMERA"
			],
			"package": "com.belshoredrive.app"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-font",
				{
					"fonts": [
						"./assets/fonts/Poppins-Regular.ttf",
						"./assets/fonts/Poppins-Bold.ttf",
						"./assets/fonts/Poppins-Medium.ttf",
						"./assets/fonts/Poppins-SemiBold.ttf"
					]
				}
			],
			[
				"expo-contacts",
				{
					"contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
				}
			],
			[
				"expo-image-picker",
				{
					"photosPermission": "The app needs access to your photos to let you change your profile picture."
				}
			],
			[
				"expo-camera",
				{
					"cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
				}
			],
			[
				"expo-notifications",
				{
					"icon": "./assets/images/icon.png",
					"color": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "3b24992d-22a3-4074-bb4d-2e3c65d6edb0"
			}
		},
		"owner": "belshoredrive"
	}
}
